{
  "name": "@gerhobbelt/jscodeshift",
  "version": "0.3.32-3",
  "description": "A toolkit for JavaScript codemods",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/jscodeshift.git"
  },
  "bugs": "https://github.com/facebook/jscodeshift/issues",
  "main": "index.js",
  "scripts": {
    "build": "npm run cp2dist && npm test",
    "cp2dist": "rm -rf dist && cp -R src/ dist/",
    "test": "jest --bail",
    "docs": "rm -rf docs && jsdoc -d docs -R README.md src/collections/*.js src/core.js src/Collection.js",
    "pub": "npm publish --access public"
  },
  "bin": {
    "jscodeshift": "./bin/jscodeshift.sh"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "codemod",
    "recast"
  ],
  "author": "Felix Kling",
  "license": "BSD-3-Clause",
  "dependencies": {
    "async": "2.6.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "babylon": "6.18.0",
    "colors": "1.1.2",
    "flow-parser": "0.61.0",
    "lodash": "4.17.4",
    "micromatch": "4.0.0",
    "node-dir": "0.1.17",
    "@gerhobbelt/nomnom": "1.8.4-24",
    "@gerhobbelt/recast": "0.13.0-22",
    "temp": "0.8.3",
    "write-file-atomic": "2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "eslint": "4.13.1",
    "jest": "22.0.1",
    "jsdoc": "3.5.5",
    "mkdirp": "0.5.1"
  },
  "jest": {
    "roots": [
      "src",
      "bin",
      "sample"
    ]
  }
}
